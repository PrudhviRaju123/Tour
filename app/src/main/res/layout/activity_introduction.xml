<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    android:id="@+id/rltivelt">


    <de.hdodenhof.circleimageview.CircleImageView xmlns:app="http://schemas.android.com/apk/res-auto"
        android:id="@+id/circularimage"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:scaleX="0.8541"
        android:scaleY="0.8541"
        android:layout_centerInParent="true"
        android:scaleType="centerCrop" />

<com.ua.hower.house.CircularSeekBar
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:layout_centerHorizontal="true"
    android:layout_centerInParent="true"
    android:layout_centerVertical="true"
    app:pointer_color="@color/white"
    app:pointer_halo_color="@color/myPrimaryColor"
    android:id="@+id/circularbar"
    app:circle_progress_color="#0174DF"
    app:circle_color="@color/half_black"
    android:scaleType="centerCrop"/>


    <com.getbase.floatingactionbutton.FloatingActionButton xmlns:app="http://schemas.android.com/apk/res-auto"
        android:id="@+id/multiple_actions"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        app:fab_addButtonColorNormal="@color/myPrimaryColor"
        app:fab_icon="@drawable/ic_play_circle_outline_white_48dp"
        app:fab_addButtonColorPressed="@color/ButtonClickColor"
        app:fab_addButtonPlusIconColor="@color/white"
        app:fab_colorPressed="@color/ButtonClickColor"
        android:layout_marginBottom="16dp"
        android:layout_marginRight="16dp"
        android:layout_marginEnd="16dp"
        android:layout_gravity="right|bottom"></com.getbase.floatingactionbutton.FloatingActionButton>




</RelativeLayout>